  "keywords": [
    "zero-to-prod",
    "JsonSchema4",
    "json-schema4"
  ],
  "homepage": "https://github.com/zero-to-prod/json-schema4",
  "license": "MIT",
  "authors": [
    {
      "name": "David Smith",
      "email": "dave0016@gmail.com"
    }
  ],
  "require": {
